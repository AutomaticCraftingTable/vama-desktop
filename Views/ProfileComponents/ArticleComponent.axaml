<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:components="clr-namespace:VamaDesktop.Views.Components"
             x:Class="VamaDesktop.Views.ProfileComponents.ArticleComponent"
             x:CompileBindings="False"
             x:Name="Self">
    <components:Box
        DropdownItems="{Binding #Self.DropdownItems}"
        Text="{Binding #Self.Article.Title}"
        ImageUrl="{Binding #Self.Article.Thumbnail}" />
</UserControl>